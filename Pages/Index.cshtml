@page
@model PowerShellWebApp.Pages.IndexModel
@{
    ViewData["Title"] = "Home page";
}
<div class="container div-center" style="width: 500px; padding-top: 50px">
<div style="text-align: center">
    <img src="data:image/png;base64," alt="Hanover-Insurance">
</div>

<div class="text-center">
    <h1 class="display-4">Agent MFA Validation</h1>
    <p>Enter your input:</p>
    <form method="post">
        <input type="text" asp-for="UserInput" />
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

    @if (Model.ScriptOutput != null)
    {
        <div class="alert alert-info mt-3">
            <strong>Script Output:</strong> <pre>@Model.ScriptOutput</pre>
        </div>
    }
</div>